extends layout.pug 
include utils.pug 

block content
    +validationErrorSummary(errors)
    form(action="/users/login", method="post")
        input(type="hidden" name="_csrf" value=csrfToken)
        +field("Username", "userName",  "")
        +field("Password", "password", "", "password" ) 
        button(type="submit") Submit